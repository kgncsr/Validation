@model Register

<div class="bg-primary text-white m-1 p-1">
    <h2>Success</h2>
</div>

<table class="table table-striped">

    <tr>
        <th>Name</th>
        <td>@Model.Name</td>
    </tr>

    <tr>
        <th>User Name</th>
        <td>@Model.UserName</td>
    </tr>

    <tr>
        <th>Mail</th>
        <td>@Model.Email</td>
    </tr>


    <tr>
        <th>BirthDate</th>
        <td>@Model.BirthDate.ToString("d")</td>
    </tr>



</table>